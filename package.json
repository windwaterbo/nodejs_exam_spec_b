    "koa-bodyparser": "^4.3.0",
    "koa-static": "^5.0.0",
    "pg": "^8.0.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.0",
    "swagger-jsdoc": "^6.2.0",
    "swagger-ui-dist": "^4.15.0",
    "tsconfig-paths": "^4.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.0.0",
